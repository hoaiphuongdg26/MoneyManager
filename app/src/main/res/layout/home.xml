<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:weightSum="105"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="30">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <ImageView
                    android:id="@+id/logo"
                    android:layout_width="100dp"
                    android:layout_height="32dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:background="@drawable/logo"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="10dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="@id/menu"
                    />
                <ImageButton
                    android:id="@+id/menu"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_alignParentTop="true"
                    android:background="@drawable/menu_img"
                    android:contentDescription="Menu"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="10dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    />
                <TextView
                    android:id="@+id/textview_welcome"
                    android:text="HI USER"
                    android:layout_marginTop="18dp"
                    android:fontFamily="@font/sf_pro_text_bold"
                    android:textSize="20sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/menu"
                    app:layout_constraintStart_toStartOf="@id/logo"
                    android:textColor="@color/blue"
                    />
                <EditText
                    android:layout_width="0dp"
                    android:layout_height="48dp"
                    android:id="@+id/edittext_search"
                    app:layout_constraintStart_toEndOf="@id/textview_welcome"
                    app:layout_constraintTop_toTopOf="@id/textview_welcome"
                    app:layout_constraintBottom_toBottomOf="@id/textview_welcome"
                    android:background="@drawable/search_bar"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:drawableStart="@drawable/kichlup"
                    android:hint=" Search..."
                    android:paddingLeft="10dp"
                    android:fontFamily="@font/sf_pro_text_regular"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toStartOf="@id/notification"
                    />
                <ImageButton
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:id="@+id/notification"
                    android:scaleType="fitCenter"
                    android:contentDescription="Menu"
                    android:background="@drawable/notification"
                    app:layout_constraintTop_toTopOf="@id/edittext_search"
                    app:layout_constraintRight_toRightOf="@id/menu"
                    />
                <TextView
                    android:id="@+id/textview_rate"
                    android:text="Income Rate"
                    android:layout_marginTop="30dp"
                    android:fontFamily="@font/sf_pro_text_regular"
                    android:textSize="16sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/textview_welcome"
                    app:layout_constraintLeft_toLeftOf="@id/textview_welcome"
                    android:layout_marginLeft="20dp"
                    android:textColor="@color/black"
                    />

                <ProgressBar
                    android:id="@+id/progressBar"
                    style="?android:attr/progressBarStyleHorizontal"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    app:layout_constraintTop_toBottomOf="@id/textview_rate"
                    android:layout_marginTop="10dp"
                    app:layout_constraintLeft_toLeftOf="@id/textview_rate"
                    app:layout_constraintRight_toRightOf="@id/notification"
                    />
                <TextView
                    android:id="@+id/textview_rateMin"
                    android:text="$0"
                    android:fontFamily="@font/sf_pro_text_regular"
                    android:textSize="12sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/light_gray"
                    android:layout_marginTop="5dp"
                    app:layout_constraintTop_toBottomOf="@id/progressBar"
                    app:layout_constraintLeft_toLeftOf="@id/progressBar"
                    />
                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/guideline"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.5" />

                <TextView
                    android:id="@+id/textview_rateMid"
                    android:text="$10k"
                    android:textSize="12sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/light_gray"
                    app:layout_constraintTop_toTopOf="@id/textview_rateMin"
                    app:layout_constraintStart_toStartOf="@id/guideline" />
                <TextView
                    android:id="@+id/textview_rateMax"
                    android:text="$27k"
                    android:fontFamily="@font/sf_pro_text_regular"
                    android:textSize="12sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/light_gray"
                    app:layout_constraintRight_toRightOf="@id/progressBar"
                    app:layout_constraintTop_toTopOf="@id/textview_rateMin"
                    />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="30"
            android:weightSum="30"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="11.5">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/textview_infoTransactions"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:fontFamily="@font/sf_pro_text_bold"
                        android:text="Recent transactions"
                        android:textColor="@color/blue"
                        android:textSize="16sp"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <Spinner
                        android:id="@+id/periodically"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/sf_pro_text_regular"
                        app:layout_constraintLeft_toLeftOf="@id/textview_infoTransactions"
                        app:layout_constraintTop_toBottomOf="@id/textview_infoTransactions" />
                </androidx.constraintlayout.widget.ConstraintLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="5.5"></LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="13">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ImageButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/historicalDetail"
                        android:contentDescription="Historical documentation detail"
                        android:background="@drawable/right_forward"
                        app:layout_constraintRight_toRightOf="parent"
                        android:layout_marginRight="10dp"
                        app:layout_constraintTop_toTopOf="@id/textview_historical"
                        app:layout_constraintBottom_toBottomOf="@id/textview_historical"
                        />
                    <TextView
                        android:id="@+id/textview_historical"
                        android:text="Historical documentation"
                        android:fontFamily="@font/sf_pro_text_regular"
                        android:textSize="13sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/light_blue"
                        android:clickable="true"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@id/historicalDetail"
                        android:layout_marginRight="5dp"
                        />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="33">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <TextView
                    android:id="@+id/textview_operation"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:fontFamily="@font/sf_pro_text_bold"
                    android:text="Operation"
                    android:textColor="@color/blue"
                    android:textSize="16sp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageButton
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:id="@+id/button_income"
                    android:contentDescription="Income/Expense"
                    android:background="@drawable/income"
                    app:layout_constraintTop_toBottomOf="@id/textview_operation"
                    app:layout_constraintLeft_toLeftOf="parent"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="10dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textview_income"
                    android:fontFamily="@font/sf_pro_text_regular"
                    android:text="Income/Expanse"
                    android:textColor="@color/light_blue"
                    android:textSize="14sp"
                    app:layout_constraintTop_toBottomOf="@id/button_income"
                    app:layout_constraintLeft_toLeftOf="@id/button_income"
                    app:layout_constraintRight_toRightOf="@id/button_income"
                    >
                </TextView>
                <ImageButton
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:id="@+id/button_plan"
                    android:contentDescription="Plan"
                    android:background="@drawable/plan"
                    app:layout_constraintStart_toEndOf="@id/button_income"
                    app:layout_constraintTop_toTopOf="@id/button_income"
                    app:layout_constraintBottom_toBottomOf="@id/button_income"
                    android:layout_marginLeft="35dp"
                    app:layout_constraintEnd_toStartOf="@id/button_statistic"
                    android:layout_marginRight="35dp"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textview_plan"
                    android:fontFamily="@font/sf_pro_text_regular"
                    android:text="Calander"
                    android:textColor="@color/light_blue"
                    android:textSize="14sp"
                    app:layout_constraintTop_toTopOf="@id/textview_income"
                    app:layout_constraintLeft_toLeftOf="@id/button_plan"
                    app:layout_constraintRight_toRightOf="@id/button_plan">
                </TextView>
                <ImageButton
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:id="@+id/button_statistic"
                    android:contentDescription="Statistic"
                    android:background="@drawable/statistic"
                    app:layout_constraintTop_toTopOf="@id/button_income"
                    app:layout_constraintBottom_toBottomOf="@id/button_income"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginRight="30dp"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textview_statistic"
                    android:fontFamily="@font/sf_pro_text_regular"
                    android:text="Statistic"
                    android:textColor="@color/light_blue"
                    android:textSize="14sp"
                    app:layout_constraintTop_toTopOf="@id/textview_plan"
                    app:layout_constraintLeft_toLeftOf="@id/button_statistic"
                    app:layout_constraintRight_toRightOf="@id/button_statistic">
                </TextView>
                <ImageButton
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:id="@+id/button_more"
                    android:contentDescription="More option"
                    android:background="@drawable/more"
                    app:layout_constraintTop_toBottomOf="@id/textview_income"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toEndOf="@id/button_income"
                    app:layout_constraintEnd_toStartOf="@id/button_plan"
                    />
                <ImageButton
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:id="@+id/button_setting"
                    android:contentDescription="Setting"
                    android:background="@drawable/setting"
                    app:layout_constraintTop_toTopOf="@id/button_more"
                    app:layout_constraintBottom_toBottomOf="@id/button_more"
                    app:layout_constraintStart_toEndOf="@id/button_plan"
                    app:layout_constraintEnd_toStartOf="@id/button_statistic"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textview_more"
                    android:fontFamily="@font/sf_pro_text_regular"
                    android:text="More"
                    android:textColor="@color/light_blue"
                    android:textSize="14sp"
                    app:layout_constraintTop_toBottomOf="@id/button_more"
                    app:layout_constraintLeft_toLeftOf="@id/button_more"
                    app:layout_constraintRight_toRightOf="@id/button_more">
                </TextView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textview_setting"
                    android:fontFamily="@font/sf_pro_text_regular"
                    android:text="Setting"
                    android:textColor="@color/light_blue"
                    android:textSize="14sp"
                    app:layout_constraintTop_toTopOf="@id/textview_more"
                    app:layout_constraintLeft_toLeftOf="@id/button_setting"
                    app:layout_constraintRight_toRightOf="@id/button_setting">
                </TextView>
            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="12"
            android:background="@drawable/bg_footer"
